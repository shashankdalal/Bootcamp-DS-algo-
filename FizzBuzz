//print 1 to n , multiples of 3=fizz and multiples of 5=buzz,for both common multiples=fizzbuzz
//1
//2
//fizz
//4
//buzz

function fizzbuzz(n)
{
  for(let i=1;i<=n;i++){
    if(i%3===0 && i%5===0)
    {
      console.log('fizzbuzz');
    }else if(i%3===0){
      console.log('fizz');
    }else if(i%5==0){
      console.log('buzz');
    }
    else{
    console.log(i);
    }
  }
}
